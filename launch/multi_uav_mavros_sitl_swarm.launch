<?xml version="1.0"?>
<launch>
    <!-- MAVROS posix SITL environment launch script -->
    <!-- launches Gazebo environment and 2x: MAVROS, PX4 SITL, and spawns vehicle -->
    <!-- vehicle model and world -->
    <arg name="est" default="ekf2"/>
    <arg name="vehicle" default="iris"/>
    <arg name="world" default="$(find mavlink_sitl_gazebo)/worlds/empty.world"/>
    <!-- gazebo configs -->
    <arg name="gui" default="true"/>
    <arg name="debug" default="false"/>
    <arg name="verbose" default="false"/>
    <arg name="paused" default="false"/>
    <!-- Gazebo sim -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="gui" value="$(arg gui)"/>
        <arg name="world_name" value="$(arg world)"/>
        <arg name="debug" value="$(arg debug)"/>
        <arg name="verbose" value="$(arg verbose)"/>
        <arg name="paused" value="$(arg paused)"/>
    </include>
    <!-- UAV1 -->
    <group ns="uav1">
        <!-- MAVROS and vehicle configs -->        
        <arg name="ID" value="1"/>
		<arg name="udp_offboard_port_remote" value="14540" />	
		<arg name="udp_offboard_port_local"  value="14557" />												<!-- Tested, and it works in these numbers -->
		<arg name="udp_gcs_port_local"       value="14557" />												<!-- 本来gcs的端口号应该是offboard的+1, 这边不知道为什么+1了反而出错，这样是能工作的 -->
        <arg name="fcu_url" default="udp://:14540@localhost:14557"/>
        <!-- PX4 SITL and vehicle spawn -->
        <include file="$(find px4)/launch/single_vehicle_spawn_swarm.launch">
            <arg name="x" value="0"/>
            <arg name="y" value="0"/>
            <arg name="z" value="0"/>
            <arg name="R" value="0"/>
            <arg name="P" value="0"/>
            <arg name="Y" value="0"/>
            <arg name="vehicle" value="$(arg vehicle)"/>
            <arg name="mavlink_udp_port" value="15561"/>
            <arg name="ID" value="$(arg ID)"/>
			<arg name="udp_offboard_port_remote" value="$(arg udp_offboard_port_remote)" />
			<arg name="udp_offboard_port_local"  value="$(arg udp_offboard_port_local)"  />					<!-- the port number here can be set larger for avoiding being the same as mavlink port numbers above -->
			<arg name="udp_gcs_port_local"       value="$(arg udp_gcs_port_local)"       />					<!-- 和仿真器连接的端口可以随意，稍微设置大一点，这样不会和mavlink的端口冲突 -->
        </include>
		<!-- Control Node -->
		<node name="offboard_node" pkg="offboard_node" type="main" output="screen"/>		
        <!-- MAVROS -->
        <include file="$(find mavros)/launch/px4.launch">
            <arg name="fcu_url" value="$(arg fcu_url)"/>
            <arg name="gcs_url" value=""/>
            <arg name="tgt_system" value="$(arg ID)"/>
            <arg name="tgt_component" value="1"/>			
        </include>
    </group>
    <!-- UAV2 -->
    <group ns="uav2">
        <!-- MAVROS and vehicle configs -->
        <arg name="ID" value="2"/>
		<arg name="udp_offboard_port_remote" value="14541" />	
		<arg name="udp_offboard_port_local"  value="14558" />												<!-- The port number here must be sequential (i.e. uav2 = uav1 + 1), or it will be some errors -->
		<arg name="udp_gcs_port_local"       value="14558" />												<!-- 这里的端口号必须和上面设置的是连号的，否则会出错 -->
        <arg name="fcu_url" default="udp://:14541@localhost:14558"/>
        <!-- PX4 SITL and vehicle spawn -->
        <include file="$(find px4)/launch/single_vehicle_spawn_swarm.launch">
            <arg name="x" value="1"/>
            <arg name="y" value="0"/>
            <arg name="z" value="0"/>
            <arg name="R" value="0"/>
            <arg name="P" value="0"/>
            <arg name="Y" value="0"/>
